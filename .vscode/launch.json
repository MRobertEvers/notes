    "version": "0.2.0",
    "configurations": [
        {
            "name": "(lldb) Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/async_implementations/async_cpp_coroutines/a.out",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [],
            "externalConsole": false,

            "MIMode": "lldb"
        },
    
        {
            "name": "Launch",
            "type": "lldb",
            "request": "launch",
            "program": "${workspaceFolder}/async_implementations/async_rust/target/debug/async_rust",
            "stopOnEntry": true,
            "args": [],
        },
        {
            "name": "Launch C",
            "type": "lldb",
            "request": "launch",
            "program": "${workspaceFolder}/virtual_dispatch/c_rust_impl_test/a.out",
            "args": [],
        },



        {
            "type": "pwa-node",
            "request": "launch",
            "name": "Launch Node",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/async_typescript/build/main.js",
            "outFiles": [
                "${workspaceFolder}/**/*.js"
            ]
        }
    ]
}